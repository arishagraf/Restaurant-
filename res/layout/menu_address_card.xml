<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <View
        android:id="@+id/rectangle_1"
        android:layout_width="11dp"
        android:layout_height="10dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="58dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="4dp"
        android:background="@drawable/rectanglecheck_1"
        />
    <TextView
        android:id="@+id/antony"
        android:layout_width="49dp"
        android:layout_height="18dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="79dp"
        android:layout_alignParentTop="true"
        android:text="@string/antony"
        android:textAppearance="@style/antony"
        android:gravity="top"
        />
    <TextView
        android:id="@+id/xyz_apartme"
        android:layout_width="275dp"
        android:layout_height="84dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="80dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="20dp"
        android:text="@string/xyz_apartme"
        android:textAppearance="@style/xyz_apartme"
        android:gravity="top"
        />
    <View
        android:id="@+id/rectangle_2"
        android:layout_width="39dp"
        android:layout_height="15dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="345dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="3dp"
        android:background="@drawable/rectanglead2"
        />
    <TextView
        android:id="@+id/home"
        android:layout_width="32dp"
        android:layout_height="15dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="349dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="3dp"
        android:text="Office"
        android:textAppearance="@style/home"
        android:gravity="top"
        />
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="79dp"
        android:layout_alignParentTop="true">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="18dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="60dp"
            android:text="Mobile: "
            android:textAppearance="@style/mobile_123_"
            android:gravity="top"
            />
        <TextView
            android:id="@+id/mobile_123_"
            android:layout_width="154dp"
            android:layout_height="18dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="60dp"
            android:text="+123-456-789"
            android:textAppearance="@style/mobile_123_"
            android:gravity="top"
            />
    </LinearLayout>
    <TextView
        android:id="@+id/remove"
        android:layout_width="48dp"
        android:layout_height="15dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="296dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="72dp"
        android:text="@string/remove"
        android:textAppearance="@style/remove"
        android:gravity="top"
        />
    <View
        android:id="@+id/line_33"
        android:layout_width="10dp"
        android:layout_height="0dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="350dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="84dp"
        android:background="@drawable/line_33"
        />
    <TextView
        android:id="@+id/edit"
        android:layout_width="25dp"
        android:layout_height="15dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="356dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="72dp"
        android:text="@string/edit"
        android:textAppearance="@style/edit"
        android:gravity="top"
        />
</RelativeLayout>

</LinearLayout>